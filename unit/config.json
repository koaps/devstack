{
    "listeners": {
        "*:80": {
            "pass": "routes"
        },
        "*:9080": {
            "pass": "applications/fapi"
        }
    },
    "applications": {
        "fapi": {
            "type": "python 3.12",
            "path": "/www/fapi_app/",
            "home": "/www/fapi_app/venv/",
            "module": "asgi",
            "callable": "app"
        }
    },
    "routes": [
        {
            "action": {
                "share": "/www/static/$uri"
            }
        }
    ]
}


